{
  "name": "@quarkloop/quarkloop",
  "version": "0.0.0",
  "author": "Quarkloop",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "pnpm --filter ./apps/quarkloop-web run dev",
    "build": "pnpm --filter ./apps/quarkloop-web run build",
    "start": "pnpm --filter ./apps/quarkloop-web run start",
    "lint": "pnpm --filter ./apps/quarkloop-web run lint",
    "studio:dev": "dotenv -e .env.development -- prisma studio",
    "studio:prod": "dotenv -e .env.production -- prisma studio",
    "db:reset": "dotenv -e .env.development -- npx prisma migrate reset --preview-feature",
    "db:migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "db:seed": "dotenv -e .env.development -- bash prisma/seed/seed.bash",
    "db:deploy": "dotenv -e .env.production -- npx prisma migrate deploy --preview-feature"
  },
  "devDependencies": {
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-custom": "*",
    "prettier": "^2.8.8",
    "typescript": "5.1.3",
    "turbo": "latest"
  },
  "dependencies": {
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "next": "^13.5.3",
    "next-transpile-modules": "^10.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@8.6.12"
}
