{
  "name": "@quarkloop/client",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "studio:dev": "dotenv -e .env.development -- prisma studio",
    "studio:prod": "dotenv -e .env.production -- prisma studio",
    "db:reset": "dotenv -e .env.development -- npx prisma migrate reset --preview-feature",
    "db:migrate": "dotenv -e .env.development -- npx prisma migrate dev",
    "db:seed": "dotenv -e .env.development -- bash prisma/seed/seed.bash",
    "db:test": "dotenv -e .env.development -- bash prisma/tests/test.bash",
    "db:deploy": "dotenv -e .env.production -- npx prisma migrate deploy --preview-feature"
  },
  "devDependencies": {
    "@quarkloop/tsconfig": "workspace:*",
    "postcss": "8.4.24",
    "postcss-preset-mantine": "^1.8.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "5.1.3"
  },
  "dependencies": {
    "@mantine/core": "^7.1.2",
    "@mantine/dates": "^7.1.2",
    "@mantine/form": "^7.1.2",
    "@mantine/hooks": "^7.1.2",
    "@mantine/modals": "^7.1.2",
    "@mantine/notifications": "^7.1.2",
    "@quarkloop/components": "workspace:*",
    "@quarkloop/lib": "workspace:*",
    "@quarkloop/apps": "workspace:*",
    "@quarkloop/types": "workspace:*",
    "@quarkloop/ui": "workspace:*",
    "@reduxjs/toolkit": "^1.9.5",
    "@tabler/icons-react": "^2.38.0",
    "autoprefixer": "10.4.14",
    "axios": "^1.4.0",
    "class-variance-authority": "^0.6.1",
    "clsx": "^1.2.1",
    "cmdk": "^0.2.0",
    "dayjs": "^1.11.8",
    "encoding": "^0.1.13",
    "lucide-react": "^0.259.0",
    "match-sorter": "^6.3.1",
    "moment": "^2.29.4",
    "next": "^13.5.3",
    "next-transpile-modules": "^10.0.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-redux": "^8.1.0",
    "tailwind-merge": "^1.13.2",
    "tailwindcss": "3.3.2",
    "tailwindcss-animate": "^1.0.6"
  }
}
