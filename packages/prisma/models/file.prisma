model AppFileSettings {
    id String @id

    enable    Boolean
    createdAt DateTime? @default(now())
    updatedAt DateTime?

    app   App    @relation(fields: [appId], references: [id], onUpdate: Cascade, onDelete: Cascade)
    appId String @unique

    @@schema("app")
}

//@relations
model App {
    id String @id

    fileSettings AppFileSettings[]
}

//@relations
model AppInstance {
    id String @id

    threadData AppFile[]
}
