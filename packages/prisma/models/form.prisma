model AppFormSettings {
    id String @id

    name       String
    fields     Json[]
    fieldCount Int?
    createdAt  DateTime? @default(now())
    updatedAt  DateTime?

    app   App    @relation(fields: [appId], references: [id], onUpdate: Cascade, onDelete: Cascade)
    appId String

    @@schema("app")
}

//@relations
model App {
    id String @id

    formSettings AppFormSettings[]
}
