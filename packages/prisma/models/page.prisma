model AppPageSettings {
    id String @id

    name       String
    entryPoint Boolean
    content    String
    createdAt  DateTime? @default(now())
    updatedAt  DateTime?

    app   App    @relation(fields: [appId], references: [id], onUpdate: Cascade, onDelete: Cascade)
    appId String

    @@schema("app")
}

//@relations
model App {
    id String @id

    pageSettings AppPageSettings[]
}
